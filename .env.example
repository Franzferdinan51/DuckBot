# Discord Configuration (REQUIRED)
DISCORD_TOKEN="your_discord_bot_token_here"

# Neo4j Database Configuration (OPTIONAL - for analytics/memory features)
NEO4J_ENABLED=true
NEO4J_URI=neo4j://127.0.0.1:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_neo4j_password_here

# ComfyUI Configuration (OPTIONAL - for image/video generation)
COMFYUI_URL=http://127.0.0.1:8188

# LM Studio Configuration (OPTIONAL - for AI chat)
LM_STUDIO_URL=http://localhost:1234/v1/chat/completions
LM_STUDIO_MODEL=auto
USE_LM_STUDIO_SYSTEM_PROMPT=true

# Global Admin Configuration (OPTIONAL)
GLOBAL_ADMIN_ID=your_discord_user_id_here